<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainFragment.HomeFragment">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_H_Top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.17"/>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_H_Middle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.27"/>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_V_L"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.10"/>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_V_R"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.90"/>

        <ImageButton
            android:id="@+id/btn_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="0dp"
            android:minHeight="0dp"
            android:src="@drawable/ic_baseline_export_24"
            android:text="1"
            app:layout_constraintLeft_toLeftOf="@id/guideline_V_L"
            app:layout_constraintRight_toLeftOf="@id/btn_02"
            app:layout_constraintTop_toTopOf="@id/guideline_H_Top" />
        <ImageButton
            android:id="@+id/btn_02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2"
            android:minHeight="0dp"
            android:minWidth="0dp"
            app:layout_constraintTop_toTopOf="@id/guideline_H_Top"
            app:layout_constraintLeft_toRightOf="@id/btn_01"
            app:layout_constraintRight_toLeftOf="@id/btn_03"
            />
        <ImageButton
            android:id="@+id/btn_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:minHeight="0dp"
            android:minWidth="0dp"
            app:layout_constraintTop_toTopOf="@id/guideline_H_Top"
            app:layout_constraintLeft_toRightOf="@id/btn_02"
            app:layout_constraintRight_toRightOf="@id/guideline_V_R"
            />
        <ImageButton
            android:id="@+id/btn_04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4"
            android:minHeight="0dp"
            android:minWidth="0dp"
            app:layout_constraintLeft_toLeftOf="@id/guideline_V_L"
            app:layout_constraintRight_toLeftOf="@id/btn_05"
            app:layout_constraintTop_toTopOf="@id/guideline_H_Middle"
            />
        <ImageButton
            android:id="@+id/btn_05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5"
            android:minHeight="0dp"
            android:minWidth="0dp"
            app:layout_constraintTop_toTopOf="@id/guideline_H_Middle"
            app:layout_constraintLeft_toRightOf="@id/btn_04"
            app:layout_constraintRight_toLeftOf="@id/btn_06"
            />
        <ImageButton
            android:id="@+id/btn_06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="6"
            android:minHeight="0dp"
            android:minWidth="0dp"
            app:layout_constraintTop_toTopOf="@id/guideline_H_Middle"
            app:layout_constraintLeft_toRightOf="@id/btn_05"
            app:layout_constraintRight_toRightOf="@id/guideline_V_R"
            />

        <TextView
            android:id="@+id/home_fragment_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hi"
            android:textColor="@color/black"
            android:textSize="26dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.604">

        </TextView>










    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>