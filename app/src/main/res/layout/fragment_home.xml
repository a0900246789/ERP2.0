<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainFragment.HomeFragment">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_H_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.10"/>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_H_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.22"/>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_H_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.32"/>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_H_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.42"/>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_H_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.52"/>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_H_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.62"/>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_H_7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.72"/>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_H_8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.82"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_V_L"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.10"/>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_V_R"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.90"/>

        <ImageButton
            android:id="@+id/btn_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="0dp"
            android:minHeight="0dp"
            android:background="@drawable/btn_01"
            app:layout_constraintLeft_toLeftOf="@id/guideline_V_L"
            app:layout_constraintRight_toLeftOf="@id/btn_02"
            app:layout_constraintTop_toTopOf="@id/guideline_H_1" />
        <ImageButton
            android:id="@+id/btn_02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minHeight="0dp"
            android:minWidth="0dp"
            android:background="@drawable/btn_02"
            app:layout_constraintTop_toTopOf="@id/guideline_H_1"
            app:layout_constraintLeft_toRightOf="@id/btn_01"
            app:layout_constraintRight_toLeftOf="@id/btn_03"
            />
        <ImageButton
            android:id="@+id/btn_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minHeight="0dp"
            android:minWidth="0dp"
            android:background="@drawable/btn_03"
            app:layout_constraintTop_toTopOf="@id/guideline_H_1"
            app:layout_constraintLeft_toRightOf="@id/btn_02"
            app:layout_constraintRight_toRightOf="@id/guideline_V_R"
            />
        <ImageButton
            android:id="@+id/btn_04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4"
            android:minHeight="0dp"
            android:minWidth="0dp"
            android:src="@drawable/pur04"
            app:layout_constraintLeft_toLeftOf="@id/guideline_V_L"
            app:layout_constraintRight_toLeftOf="@id/btn_05"
            app:layout_constraintTop_toTopOf="@id/guideline_H_2"
            />
        <ImageButton
            android:id="@+id/btn_05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5"
            android:minHeight="0dp"
            android:minWidth="0dp"
            android:src="@drawable/ops05"
            app:layout_constraintTop_toTopOf="@id/guideline_H_2"
            app:layout_constraintLeft_toRightOf="@id/btn_04"
            app:layout_constraintRight_toLeftOf="@id/btn_06"
            />
        <ImageButton
            android:id="@+id/btn_06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="6"
            android:minHeight="0dp"
            android:minWidth="0dp"
            android:src="@drawable/stk06"
            app:layout_constraintTop_toTopOf="@id/guideline_H_2"
            app:layout_constraintLeft_toRightOf="@id/btn_05"
            app:layout_constraintRight_toRightOf="@id/guideline_V_R"
            />

        <ImageButton
            android:id="@+id/btn_07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minHeight="0dp"
            android:minWidth="0dp"
            android:src="@drawable/pur04"
            app:layout_constraintLeft_toLeftOf="@id/guideline_V_L"
            app:layout_constraintRight_toLeftOf="@id/btn_08"
            app:layout_constraintTop_toTopOf="@id/guideline_H_3"
            />
        <ImageButton
            android:id="@+id/btn_08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minHeight="0dp"
            android:minWidth="0dp"
            android:src="@drawable/ops05"
            app:layout_constraintTop_toTopOf="@id/guideline_H_3"
            app:layout_constraintLeft_toRightOf="@id/btn_07"
            app:layout_constraintRight_toLeftOf="@id/btn_09"
            />
        <ImageButton
            android:id="@+id/btn_09"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minHeight="0dp"
            android:minWidth="0dp"
            android:src="@drawable/stk06"
            app:layout_constraintTop_toTopOf="@id/guideline_H_3"
            app:layout_constraintLeft_toRightOf="@id/btn_08"
            app:layout_constraintRight_toRightOf="@id/guideline_V_R"
            />
















    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>